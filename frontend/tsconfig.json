{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"paths": {
			"$components/*": ["src/lib/components/*"],
			"$stores/*": ["src/lib/stores/*"],
			"$services/*": ["src/lib/services/*"],
			"$types/*": ["src/lib/types/*"]
		}
	}
}